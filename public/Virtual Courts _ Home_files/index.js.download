// Enter Captcha  Added by Mohini 17.05.2025

function isAlphaNumeric(evt) {
    evt = evt || window.event;
    var charCode = evt.which || evt.keyCode;
    if (
        (charCode >= 48 && charCode <= 57) || 
        (charCode >= 65 && charCode <= 90) || 
        (charCode >= 97 && charCode <= 122)   
    ) {
        return true;
    }
    return false; 
}